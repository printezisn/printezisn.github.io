---
import BlogLayout from '@layouts/blog-layout.astro';
import { PrivacyPolicyHtml } from '@printezisn/games-couples-run/templates';
import styles from '@styles/pages/privacy-policy/styles.module.scss';

const title = 'Privacy Policy for Couples Run';
const description = 'Privacy policy for the Couples Run game';
---

<BlogLayout
  title={title}
  description={description}
  path="/games/couples-run/privacy-policy/"
  breadcrumbList={[
    {
      name: 'Portfolio',
      url: '/',
    },
    {
      name: 'Privacy Policy',
      url: '/privacy-policy/',
    },
    {
      name: 'Couples Run',
      url: '/games/couples-run/privacy-policy/',
    },
  ]}
  lastUpdateTime={new Date('2025-06-29')}
>
  <div class={styles.content}>
    <Fragment set:html={PrivacyPolicyHtml} />
  </div>
</BlogLayout>
