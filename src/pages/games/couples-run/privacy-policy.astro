---
import BlogLayout from '../../../layouts/blog-layout.astro';
import { PrivacyPolicyHtml } from '@printezisn/games-couples-run/templates';

const title = 'Privacy Policy | Couples Run';
const description = 'Privacy policy for the Couples Run game';
---

<BlogLayout
  title={title}
  description={description}
  path="/games/couples-run/privacy-policy/"
  breadcrumbList={[
    {
      name: 'Portfolio',
      url: '/',
    },
    {
      name: 'Couples Run',
      url: '/games/couples-run/',
    },
    {
      name: 'Privacy Policy',
      url: '/games/couples-run/privacy-policy/',
    },
  ]}
>
  <Fragment set:html={PrivacyPolicyHtml} />
</BlogLayout>
